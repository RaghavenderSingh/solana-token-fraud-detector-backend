{
  "name": "tokentrust-core",
  "version": "1.0.0",
  "description": "Multi-Source Intelligence for DeFi Security - AI-powered token fraud detection platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "analyze": "node dist/core/analyzer.js",
    "analyze:test": "node dist/core/test-analyzer.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "defi",
    "security",
    "solana",
    "token-analysis",
    "fraud-detection",
    "blockchain",
    "ai",
    "multi-source-intelligence"
  ],
  "author": "TokenTrust Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tokentrust/core.git"
  },
  "bugs": {
    "url": "https://github.com/tokentrust/core/issues"
  },
  "homepage": "https://tokentrust.dev",
  "dependencies": {
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "socket.io": "^4.8.1",
    "redis": "^4.6.13",
    "openai": "^4.28.0",
    "cheerio": "^1.0.0-rc.12",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^4.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.15",
    "@types/socket.io": "^3.0.1",
    "@types/redis": "^4.0.11",
    "@types/compression": "^1.7.5",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
